{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "cviebrock/eloquent-sluggable": "^12.0",
        "guzzlehttp/guzzle": "^7.10.0",
        "laravel/framework": "^12.44.0",
        "laravel/sanctum": "^4.2.1",
        "laravel/socialite": "^5.24.0",
        "laravel/tinker": "^2.10.2",
        "livewire/livewire": "^3.7.3",
        "spatie/laravel-permission": "^6.24.0",
        "spatie/laravel-tags": "^4.10.1",
        "torchlight/torchlight-laravel": "^0.6.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.8.1",
        "laravel/boost": "^1.8.7",
        "laravel/breeze": "^2.3.8",
        "laravel/pint": "^1.26.0",
        "laravel/sail": "^1.51.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.3",
        "phpunit/phpunit": "^11.5.46",
        "spatie/laravel-ignition": "^2.9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "format": [
            "./vendor/bin/pint"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
